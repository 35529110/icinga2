#include "base/configobject.hpp"

library icingadb;

namespace icinga
{

class IcingaDB : ConfigObject
{
	activation_priority 100;

	[config] String host {
		default {{{ return "127.0.0.1"; }}}
	};
	[config] int port {
		default {{{ return 6380; }}}
	};
	[config] String path;
	[config, no_user_view, no_user_modify] String password;
	[config] int db_index;
};

}
